  "scripts": {
    "start" : "gulp serve-dev",
    "bootstrap": "rm -f .git/hooks/prepare-commit-msg.sample && ln -s -f ../../githooks/prepare-commit-msg .git/hooks/prepare-commit-msg",
    "postinstall": "npm run clone-fonts && bower install && npm run bootstrap",
    "clone-fonts": "rm -rf ./app/assets/fonts/customers-use && git init ./app/assets/fonts/customers-use && cd ./app/assets/fonts/customers-use && git remote add origin https://git@bitbucket.org/flimbodev/flimbo-fonts.git && git pull origin master",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run"
  },
  "dependencies": {},
  "devDependencies": {
